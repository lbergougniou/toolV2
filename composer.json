        "guzzlehttp/guzzle": "^7.9",
        "vlucas/phpdotenv": "^5.6",
        "symfony/validator": "^7.1",
        "psr/http-message": "^1.0",
        "mailjet/mailjet-apiv3-php": "^1.6",
        "giggsey/libphonenumber-for-php": "^9.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.4",
        "php-webdriver/webdriver": "^1.15"
